{
  "name": "n8n-nodes-crownpeak-pd",
  "version": "1.3.0",
  "description": "n8n integration for Fredhopper Product Discovery API",
  "keywords": [
    "n8n",
    "n8n-nodes",
    "crownpeak",
    "pd",
    "n8n-community-node-package"
  ],
  "author": "Crownpeak Technology, Inc.",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp nodes/CrownpeakPD/crownpeak.svg dist/nodes/CrownpeakPD/",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Crownpeak/n8n-nodes-crownpeak-pd.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/nodes/CrownpeakPD/credentials/CrownpeakPDApi.credentials"
    ],
    "nodes": [
      "dist/nodes/CrownpeakPD/CrownpeakPD.node.js"
    ]
  }
}